{
    "manifest_version": 3,
    "name": "Linklib",
    "description": "Extension for Linklib",
    "version": "1.0",
    "action": {
      "default_popup": "index.html",
      "default_icon": "hello_extensions.png"
    },
    "content_scripts": [
      {
        "js": [
          "src/scripts/highlighter/component2.ts",
          "src/scripts/highlighter/controller.js",
          "src/scripts/highlighter/content.tsx"
        ],
        "matches": ["<all_urls>"]
      }
    ]
  }
  